{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Utilities/*": ["src/Utilities/*"],
      "@Types/*": ["src/Types/*"],
      "@Screens/*": ["src/Screens/*"],
      "@Routes/*": ["src/Routes/*"],
      "@Resources/*": ["src/Resources/*"],
      "@Redux/*": ["src/Redux/*"],
      "@Components/*": ["src/Components/*"],
      "@Common/*": ["src/Components/Common/*"]
    }
  }
}
